<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jp.co.senshinsoft.persistence.SupplierMapper">
<select id="supplierCatalog"
		resultType="jp.co.senshinsoft.domain.Supplier">
		SELECT
			supplier_code
			,supplier_name
		FROM
			supplier
		WHERE
			end_date='99999999'
</select>
</mapper>